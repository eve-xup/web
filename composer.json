{
    "name": "xup/web",
    "description": "Webfiles for xup project",
    "autoload": {
        "psr-4": {
            "Xup\\Web\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.54",
        "laravelevetools/eve-socialite-provider": "^1.0",
        "laravel/socialite": "^5.2",
        "xup/core": "^1.0",
        "rappasoft/laravel-livewire-tables": "^1.0",
        "josegus/laravel-flash": "^1.0",
        "rawilk/laravel-form-components": "^6.0",
        "blade-ui-kit/blade-heroicons": "dev-main",
        "livewire/livewire": "^2.6",
        "livewire-ui/modal": "^1.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../core"
        },
        {
            "type": "path",
            "url": "../../packages/LaravelEveTools/eveapi"
        },
        {
            "type": "path",
            "url": "../../packages/SimplyUnnamed/LaravelExtensibleApplication"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Xup\\Web\\WebServiceProvider",
                "Xup\\Web\\LivewireServiceProvider"
            ]
        }
    }
}
